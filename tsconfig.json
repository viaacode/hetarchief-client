{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@account/*": ["modules/account/*"],
      "@admin/*": ["modules/admin/*"],
      "@auth/*": ["modules/auth/*"],
      "@content-page/*": ["modules/content-page/*"],
      "@cookie-policy/*": ["modules/cookie-policy/*"],
      "@cp/*": ["modules/cp/*"],
      "@home/*": ["modules/home/*"],
      "@i18n/*": ["modules/i18n/*"],
      "@ie-objects/*": ["modules/ie-objects/*"],
      "@iiif-viewer/*": ["modules/iiif-viewer/*"],
      "@maintenance-alerts/*": ["modules/maintenance-alerts/*"],
      "@material-requests/*": ["modules/material-requests/*"],
      "@navigation/*": ["modules/navigation/*"],
      "@newsletter/*": ["modules/newsletter/*"],
      "@search/*": ["modules/search/*"],
      "@shared/*": ["modules/shared/*"],
      "@sitemap/*": ["modules/sitemap/*"],
      "@user-conditions/*": ["modules/user-conditions/*"],
      "@visit-requests/*": ["modules/visit-requests/*"],
      "@visitor-layout/*": ["modules/visitor-layout/*"],
      "@visitor-space/*": ["modules/visitor-space/*"],
      "react": ["../node_modules/react"]
    },
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["node"],
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": ["src", "node_modules/@types"]
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "tests/**/*.ts",
    "node_modules/vitest/globals.d.ts"
  ],
  "exclude": ["node_modules"]
}
