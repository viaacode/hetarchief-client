{
  "name": "hetarchief-client",
  "private": true,
  "contributors": [
    "Andry Charlier <andry.charlier@studiohyperdrive.be>"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "next lint",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
    "test": "jest --cache",
    "test:ci": "jest",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --cache --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "npm run i:a2-c --silent && npm run i:a2-t --silent",
    "i:a2-c": "npm --prefix node_modules/@viaa/avo2-components i node_modules/@viaa/avo2-components --legacy-peer-deps",
    "i:a2-t": "npm --prefix node_modules/@viaa/avo2-types i node_modules/@viaa/avo2-types --legacy-peer-deps"
  },
  "dependencies": {
    "@viaa/avo2-components": "^1.78.0",
    "@viaa/avo2-types": "^2.33.0",
    "classnames": "2.3.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-modal": "3.14.4"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@storybook/addon-actions": "6.4.8",
    "@storybook/addon-essentials": "6.4.8",
    "@storybook/addon-links": "6.4.8",
    "@storybook/builder-webpack5": "6.4.8",
    "@storybook/manager-webpack5": "6.4.8",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.4.8",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "@types/react": "17.0.36",
    "@types/react-modal": "3.13.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "css-loader": "6.5.1",
    "cypress": "9.1.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-storybook": "0.5.3",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "lint-staged": "12.1.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "7.0.1",
    "prettier": "2.4.1",
    "sass": "1.43.4",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.5.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
