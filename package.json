{
	"name": "hetarchief-client",
	"version": "0.4.1",
	"private": true,
	"contributors": [
		"Andry Charlier <andry.charlier@studiohyperdrive.be>"
	],
	"scripts": {
		"dev": "next dev -p 3200",
		"build": "next build",
		"start": "next start",
		"lint": "npm run lint:ts && npm run lint:scss",
		"lint:ts": "next lint --dir src",
		"lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
		"test": "jest --cache",
		"test:ci": "jest",
		"test:cov": "npm run test -- --coverage",
		"test:watch": "npm run test -- --watch",
		"test:e2e": "npx playwright test",
		"test:e2e:debug": "cross-env PWDEBUG=console dotenv -e .env.local npx playwright test --browser=firefox --headed --workers=1 --retries=0 --maxFailures=1 tests/auth/login-with-redirect.spec.ts",
		"type-check": "tsc --noEmit",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"prepare": "husky install",
		"storybook": "start-storybook -p 6006",
		"build:storybook": "build-storybook",
		"i18n:extract": "ts-node --transpile-only --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.ts && npm run lint:ts -- --fix",
		"gql:extract": "ts-node --transpile-only --require dotenv/config scripts/whitelist-gql-queries.ts dotenv_config_path=.env"
	},
